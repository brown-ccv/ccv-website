{
  "trailingComma": "es5",
  "semi": false,
  "singleQuote": false,
  "overrides": [
    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": "*.tsx",
      "options": {
        "parser": "typescript"
      }
    }
  ],
  "plugins": ["prettier-plugin-tailwindcss"]
}
